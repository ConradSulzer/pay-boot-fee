<div id="changeBootFee">
    <h4 id="currentFee">Current Fee: <%= fee %></h4>
    <h4 id="currentDeposit">Current Deposit: <%= deposit %></h4>
    <form class="partialForm" id="prices">
        <div id="resMessage"></div>
        <div class="form-group">
            <label for="fee">New Boot Fee</label>
            <input type="text" class="form-control" id="fee" name="fee" aria-describedby="new boot fee"
                placeholder="Enter new boot fee...">
        </div>
        <div class="form-group">
            <label for="deposit">New Deposit</label>
            <input type="text" class="form-control" id="deposit" name="deposit" aria-describedby="new boot deposit"
            placeholder="Enter new deposit...">
        </div>
        <button type="button" class="btn btn-primary" onclick="app.changePrice(this)">Submit</button>
    </form>
</div>